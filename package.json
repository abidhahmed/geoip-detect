{
  "name": "geoip-detect",
  "version": "2.11.0",
  "description": "Geolocation IP Detection - Wordpress Plugin (JS)",
  "repository": "git@github.com:yellowtree/geoip-detect.git",
  "author": "Benjamin Pick <benjaminpick@github.com>",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "clean": "rm -rf .cache",
    "start": "rm -rf js/dist && parcel js/frontend.js --dist-dir js/dist --public-url .",
    "build": "rm -rf js/dist && parcel build js/frontend.js --dist-dir js/dist --public-url .",
    "test": "jest --coverage",
    "update-browserlist": "browserslist --update-db"
  },
  "private": false,
  "dependencies": {
    "@babel/runtime": "7.15.4",
    "emoji-flags": "1.3.0",
    "just-compare": "1.4.0",
    "just-intersect": "3.2.0",
    "just-safe-get": "2.1.2",
    "just-safe-set": "2.2.3"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.4",
    "babel-jest": "27.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "eslint": "7.32.0",
    "jest": "27.1.1",
    "parcel": "2.0.0-rc.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": "> 0.2%, last 2 versions, not dead",
  "jest": {
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.js$": "babel-jest"
    }
  }
}
